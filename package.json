{
  "private": false,
  "description": "Stewed UI components in React.",
  "scripts": {
    "prepare": "husky install",
    "start": "turbo run start --parallel --stream",
    "build": "turbo run build",
    "test": "turbo run test --parallel",
    "lint": "turbo run lint --parallel",
    "lint:style": "turbo run lint:style --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo exec -- rm -rf node_modules && lerna exec -- rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@harmonix-hub/eslint": "^1.3.0",
    "@harmonix-hub/prettier": "^1.3.0",
    "@harmonix-hub/stylelint": "^1.3.1",
    "@harmonix-hub/tsconfig": "^1.3.0",
    "@storybook/addon-essentials": "^8.0.6",
    "@storybook/addon-interactions": "^8.0.6",
    "@storybook/addon-links": "^8.0.6",
    "@storybook/addon-onboarding": "^8.0.6",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/blocks": "^8.0.6",
    "@storybook/react": "^8.0.6",
    "@storybook/react-vite": "^8.0.6",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.2.74",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "lerna": "^8.1.2",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "semantic-release": "^23.0.7",
    "semantic-release-config-conventional": "^1.0.1",
    "storybook": "^8.0.6",
    "turbo": "^1.13.2"
  },
  "optionalDependencies": {
    "@nx/nx-linux-x64-gnu": "^18.2.2"
  },
  "packageManager": "pnpm@8.9.0"
}
