{
  "name": "@stewed/hooks",
  "description": "Stewed UI Shared hooks",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "eslint",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@stewed/tsconfig": "workspace:*",
    "@stewed/lint": "workspace:*",
    "@stewed/utils": "workspace:*",
    "@types/react": "^18.0.28",
    "react": "^18.2.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
